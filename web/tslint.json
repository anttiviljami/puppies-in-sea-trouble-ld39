{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "quotemark": [
      false,
      "single",
      "avoid-escape"
    ],
    "ordered-imports": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "no-console": false
  }
}
